# LLM Provider API Keys
OPENAI_API_KEY=sk-...
ANTHROPIC_API_KEY=sk-ant-...

# LangSmith Configuration (Cloud US default)
LANGSMITH_API_KEY=lsv2_...
LANGSMITH_PROJECT=my-agent-prod
LANGSMITH_ENDPOINT=https://api.smith.langchain.com

# Optional: required for API keys scoped to multiple workspaces
# LANGSMITH_WORKSPACE_ID=<workspace-id>

# Optional: EU region
# LANGSMITH_ENDPOINT=https://eu.api.smith.langchain.com

# Optional: self-hosted LangSmith instance (include /api/v1)
# LANGSMITH_ENDPOINT=https://<your-langsmith-host>/api/v1

# For Standalone / Self-hosted Data Plane Deployments
DATABASE_URI=postgresql://user:password@host:5432/dbname
REDIS_URI=redis://host:6379/0
LANGGRAPH_CLOUD_LICENSE_KEY=<your-license-key>

# Optional: Environment
ENVIRONMENT=production
LOG_LEVEL=INFO

# Optional: Custom Configuration
# Add your application-specific environment variables here
